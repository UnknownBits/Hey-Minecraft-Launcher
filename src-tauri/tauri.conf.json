{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "hey minecraft launcher",
  "version": "0.1.0",
  "identifier": "com.hey-minecraft-launcher.app",
  "build": {
    "frontendDist": "../src"
  },
  "app": {
    "withGlobalTauri": true,
    "windows": [
      {
        "label": "main",
        "minimizable": true,
        "dragDropEnabled": false,
        "resizable": true,
        "title": "hey minecraft launcher",
        "width": 830,
        "height": 619,
        "minWidth": 650,
        "minHeight": 500,
        "center": true,
        "decorations": false,
        "transparent": true
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
